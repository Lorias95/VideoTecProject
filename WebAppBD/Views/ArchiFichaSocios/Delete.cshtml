@model WebAppBD.Models.ArchiFichaSocio

@{
    ViewData["Title"] = "Delete";
}

    <h1>Eliminar Registro Ficha Socio</h1>

<h3>¿Está seguro de eliminar este registro?</h3>
<div>
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Género Favorito:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GeneroFavorito)
        </dd>
        <dt class="col-sm-2">
            Actor Favorito:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ActoresFavorito)
        </dd>
        <dt class="col-sm-2">
            Director Favorito:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DirectoresFavorito)
        </dd>
        <dt class="col-sm-2">
            Código Socio:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodigoSocioNavigation.Codigo_Socio)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdFichaSocio" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a class="btn btn-success" asp-action="Index">Atrás</a>
    </form>
</div>
